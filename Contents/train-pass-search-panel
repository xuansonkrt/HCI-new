var SearchTrainPass=$.extend({},new SearchProduct($("#day-pass"),"TrainPass"),{GetProducts:function(){$.ajax({url:this._$search.find("input[id*='hdApiGetProducts']").val()+"?areaCode="+this._$search.find("#ddlCountry"+this._type).val(),success:$.proxy(function(n){n.length>0&&$("div.dpt-product-search").html(n)},this)})},InitializeCountry:function(){this._$search.find("#ddlCountry"+this._type).on("change",$.proxy(function(){this.GetProducts()},this))},InitializeCurrency:function(){$("#hdnSelectedCurrency_"+this._type).val()===""&&$("a.toggle-currency").html("Currency <span class='caret'><\/span>");$("#currency-list").on("click","a",function(n){n.preventDefault();$("#hdnSelectedCurrency_TrainPass").val($(this).parent().data("value")).trigger("change")})}});$(document).ready(function(){})