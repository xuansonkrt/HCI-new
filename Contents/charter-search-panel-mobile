"use strict";function InitCharterSearchPanelMobile(){var n="1";$("#txtBoxPickUpTimeM").timepicker();$("#txtBoxReturnTimeM").timepicker();dispayOneWayModeMobile();$('input:radio[name="CharterTypeM"]').click(function(){$("#hdnRdBtn").val($(this).val());resetErrorMobile();$(this).val()=="1"?(n="1",dispayOneWayModeMobile()):$(this).val()=="2"?(n="2",displayTwoWayModeMobile()):(n="3",$("#txtBoxCharterDropOffLocationM").val(""),displayPerHourModeMobile())});$("#charterSearchFormMobile .swap a").click(function(){var t,i,r,u,f,e;(n==="1"||n==="2")&&($("#txtBoxCharterPickUpLocationM").val()==""||$("#txtBoxCharterDropOffLocationM").val()==""?(TriggerShakeAnimation($(".search-wrap")),$("#txtBoxCharterPickUpLocationM").addClass("is-error"),$("#txtBoxCharterDropOffLocationM").addClass("is-error"),console.log("error on empty location")):($("#txtBoxCharterPickUpLocationM").removeClass("is-error"),$("#txtBoxCharterDropOffLocationM").removeClass("is-error"),t=$("#txtBoxCharterPickUpLocationM").val(),i=$("#txtBoxCharterDropOffLocationM").val(),$("#txtBoxCharterPickUpLocationM").val(i),$("#txtBoxCharterDropOffLocationM").val(t)),$("#txtBoxCharterPickUpStateM").val()==""||$("#txtBoxCharterDropOffStateM").val()==""?(TriggerShakeAnimation($(".search-wrap")),$("#txtBoxCharterPickUpStateM").addClass("is-error"),$("#txtBoxCharterDropOffStateM").addClass("is-error")):($("#txtBoxCharterPickUpStateM").removeClass("is-error"),$("#txtBoxCharterDropOffStateM").removeClass("is-error"),r=$("#txtBoxCharterPickUpStateM").val(),u=$("#txtBoxCharterDropOffStateM").val(),$("#txtBoxCharterPickUpStateM").val(u),$("#txtBoxCharterDropOffStateM").val(r)),$("#txtBoxCharterPickUpCountryM").val()==""||$("#txtBoxCharterDropOffCountryM").val()==""?(TriggerShakeAnimation($(".search-wrap")),$("#txtBoxCharterPickUpCountryM").addClass("is-error"),$("#txtBoxCharterDropOffCountryM").addClass("is-error")):($("#txtBoxCharterPickUpCountryM").removeClass("is-error"),$("#txtBoxCharterDropOffCountryM").removeClass("is-error"),f=$("#txtBoxCharterPickUpCountryM").val(),e=$("#txtBoxCharterDropOffCountryM").val(),$("#txtBoxCharterPickUpCountryM").val(e),$("#txtBoxCharterDropOffCountryM").val(f)))});$("form#charterSearchFormMobile").submit(function(t){var i=!0;if($("#locationError_charterM").val(""),$("#txtBoxPickUpDateM").val()==""?($("#txtBoxPickUpDateM").addClass("is-error"),i=!1):$("#txtBoxPickUpDateM").removeClass("is-error"),$("#txtBoxPickUpTimeM").val()==""?($("#txtBoxPickUpTimeM").addClass("is-error"),i=!1):$("#txtBoxPickUpTimeM").removeClass("is-error"),n=="1"||n=="2"){if($("#txtBoxCharterPickUpLocationM").val()==""?($("#txtBoxCharterPickUpLocationM").addClass("is-error"),i=!1):$("#txtBoxCharterPickUpLocationM").removeClass("is-error"),$("#txtBoxCharterPickUpStateM").val()==""?($("#txtBoxCharterPickUpStateM").addClass("is-error"),i=!1):$("#txtBoxCharterPickUpStateM").removeClass("is-error"),$("#txtBoxCharterPickUpCountryM").val()==""?($("#txtBoxCharterPickUpCountryM").addClass("is-error"),i=!1):$("#txtBoxCharterPickUpCountryM").removeClass("is-error"),$("#txtBoxCharterDropOffLocationM").val()==""?($("#txtBoxCharterDropOffLocationM").addClass("is-error"),i=!1):$("#txtBoxCharterDropOffLocationM").removeClass("is-error"),$("#txtBoxCharterDropOffStateM").val()==""?($("#txtBoxCharterDropOffStateM").addClass("is-error"),i=!1):$("#txtBoxCharterDropOffStateM").removeClass("is-error"),$("#txtBoxCharterDropOffCountryM").val()==""?($("#txtBoxCharterDropOffCountryM").addClass("is-error"),i=!1):$("#txtBoxCharterDropOffCountryM").removeClass("is-error"),n=="2"){$("#txtBoxReturnDateM").val()==""?($("#txtBoxReturnDateM").addClass("is-error"),i=!1):$("#txtBoxReturnDateM").removeClass("is-error");$("#txtBoxReturnTimeM").val()==""?($("#txtBoxReturnTimeM").addClass("is-error"),i=!1):$("#txtBoxReturnTimeM").removeClass("is-error");var r=$("#txtBoxPickUpDateM").datepicker("getDate"),u=$.datepicker.formatDate("mm/dd/yy",r),o=$("#txtBoxReturnDateM").datepicker("getDate"),s=$.datepicker.formatDate("mm/dd/yy",o),f=$("#txtBoxPickUpTimeM").val(),h=$("#txtBoxReturnTimeM").val(),e=new Date(u+" "+f),c=new Date(s+" "+h),l=e.getTime(),a=c.getTime();a<=l?($("#txtBoxReturnDateM").addClass("is-error"),$("#txtBoxReturnTimeM").addClass("is-error"),i=!1):($("#txtBoxReturnDateM").removeClass("is-error"),$("#txtBoxReturnTimeM").removeClass("is-error"))}}else $("#txtBoxCharterPickUpLocationM").val()==""?($("#txtBoxCharterPickUpLocationM").addClass("is-error"),i=!1):$("#txtBoxCharterPickUpLocationM").removeClass("is-error");var r=$("#txtBoxPickUpDateM").datepicker("getDate"),u=$.datepicker.formatDate("mm/dd/yy",r),f=$("#txtBoxPickUpTimeM").val(),e=new Date(u+" "+f),v=Date.now()+864e5;e<v?(i=!1,$("#txtBoxPickUpDateM").addClass("is-error"),$("#txtBoxPickUpTimeM").addClass("is-error"),$("#locationError_charterM").text("*Pickup date must be 24 hours in advance")):($("#locationError_charterM").text(""),$("#txtBoxPickUpDateM").removeClass("is-error"),$("#txtBoxPickUpTimeM").removeClass("is-error"));i||t.preventDefault()})}function resetErrorMobile(){$("#txtBoxCharterPickUpLocationM").removeClass("is-error");$("#txtBoxCharterDropOffLocationM").removeClass("is-error");$("#txtBoxCharterPickUpStateM").removeClass("is-error");$("#txtBoxCharterDropOffStateM").removeClass("is-error");$("#txtBoxCharterPickUpCountryM").removeClass("is-error");$("#txtBoxCharterDropOffCountryM").removeClass("is-error");$("#txtBoxPickUpDateM").removeClass("is-error");$("#txtBoxPickUpTimeM").removeClass("is-error");$("#txtBoxReturnDateM").removeClass("is-error");$("#txtBoxReturnTimeM").removeClass("is-error")}function dispayOneWayModeMobile(){$("#dvSwapM").show();$("#dvDropOffM").show();$("#dvDropOffStateM").show();$("#dvDropOffCountryM").show();$("#dvReturnDateM").hide();$("#dvReturnTimeM").hide();$("#dvRentDurationM").hide()}function displayTwoWayModeMobile(){$("#dvSwapM").show();$("#dvDropOffM").show();$("#dvDropOffStateM").show();$("#dvDropOffCountryM").show();$("#dvReturnDateM").show();$("#dvReturnTimeM").show();$("#dvRentDurationM").hide()}function displayPerHourModeMobile(){$("#dvReturnDateM").hide();$("#dvReturnTimeM").hide();$("#dvSwapM").hide();$("#dvDropOffM").hide();$("#dvDropOffStateM").hide();$("#dvDropOffCountryM").hide();$("#dvRentDurationM").show()}function TriggerShakeAnimation(n){n.addClass("error-shake");setTimeout(function(){n.removeClass("error-shake")},300)}var charterConstant={_searchPanelFormId:"#charterSearchForm",_submitFormId:"#FormCharterSubmit",_pickupLocationId:"#txtBoxCharterPickUpLocation",_dropoffLocationId:"#txtBoxCharterDropOffLocation",_swapLocationDivId:"#dvSwap",_pickupTimeId:"#txtBoxPickUpTime",_returnTimeId:"#txtBoxReturnTime"},charterGeneral={initialiseInvalidFormControlPriority:function(n){$(n).bind("invalid-form.validate",function(n,t){var i=t.numberOfInvalids();i&&t.errorList[0].element.focus()})}};$(document).ready(function(){InitCharterSearchPanelMobile()})